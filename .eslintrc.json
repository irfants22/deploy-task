{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-var": "error",
        "no-use-before-define": "error",
        "no-unused-vars" : "warn",
        "no-unused-expressions": "warn"
    }
}
